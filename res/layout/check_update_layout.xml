<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    android:orientation="vertical" >
    
	<RelativeLayout android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:gravity="center_vertical"
	    android:background="@drawable/bg_set_cover">
	    <TextView android:id="@+id/tv_back"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="@string/setting_feedback_back"
	        android:textSize="18sp"
	        android:gravity="center_vertical"
	        android:drawableLeft="@drawable/topmenu_back_a"
	        android:textColor="@color/white"/>
	    <TextView android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="@string/setting_check_update_text"
	        android:textSize="18sp"
	        android:layout_centerHorizontal="true"
	        android:textColor="@color/white"/>
	    
	</RelativeLayout>
	
	
	
	
</LinearLayout>
